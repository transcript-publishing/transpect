<?xml version="1.0" encoding="UTF-8"?>
<c:param-set xmlns:c="http://www.w3.org/ns/xproc-step">
  <c:param name="work-regex" value="\d{5}"/>
  <c:param name="type-regex" value="[a-z]{3,4}"/>
  <c:param name="series-regex" value="[a-z]{3}"/>
  <c:param name="publisher-regex" value="[a-z]{2}"/>
  <c:param name="cascade-paths-regex" value="^{$publisher-regex}_{$series-regex}_{$type-regex}_{$work-regex}(_.*)?$"/>
  <c:param name="unzip-dirname" value="archive"/>
  <c:param name="cascade-path" value="."/>
  <c:param name="exclude-filter" value="\.svn|\.git|css|epub|evolve-hub|fonts|htmltemplates|hub2tei|jats2html|js|latex|logos|metadata|schema|tei2html|idml2xml|xml2tex|schematron|styles|xpl|xsl"/>
  <c:param name="content-base-uri" value="http://transpect.io/content-repo/"/>
  <c:param name="paths-xsl-uri" value="http://this.transpect.io/a9s/ts/xsl/paths.xsl"/>
  <c:param name="evolve-hub-driver-docx-path" value="evolve-hub/driver-docx"/>
  <c:param name="evolve-hub-driver-idml-path" value="evolve-hub/driver-idml"/>
  <c:param name="clade-role" value="publisher"/>
  <c:param name="rule-category-span-class" value="category"/>
  <c:param name="interface-language" value="de"/>
  <c:param name="epub-version" value="EPUB3"/>
  <c:param name="epub-a11y-check" value="yes"/>
  <c:param name="epub-a11y-ace-path" value="/usr/local/lib/node_modules/@daisy/ace/bin/ace-puppeteer.js"/>
  <c:param name="epub-a11y-htmlreport" value="yes"/>
  <c:param name="epub-a11y-rule-family-name" value="Barrierefreiheit"/>
  <c:param name="epub-a11y-severity-override" value="info"/>  
  
</c:param-set>
