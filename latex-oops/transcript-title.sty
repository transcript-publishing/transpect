\def\tp@create@editor@string#1{%
  % Name lists need further expansion than \tpIfComp to detect \relax correctly.
  \expandafter\expandafter\expandafter\ifx\csname tp@\tp@cur@cont @#1NameList\endcsname\relax%
    % This assignment is needed for \tpIfComp{#1NameList}{…}{…} to work correctly for non-existing NameLists.
    \global\expandafter\let\csname tp@\tp@cur@cont @#1NameList\endcsname\relax%
  \else%
    \def\@tempa{Author}%
    \edef\@argi{#1}%
    \ifx\@tempa\@argi%
    \else%
      \letcs\tpTotalCount{tp#1Cnt}%
      \expandafter\xdef\csname tp@\tp@cur@cont @#1NameList\endcsname{%
        % 14238: Umbrüche vor Herausgeberkürzel verhindern.
        \tpUseComp{#1NameList}~\tpUseProperty{editor-suffix}%
      }%
    \fi%
  \fi%
}%

% Keine Zeilenumbrüche vor dem Herausgebersuffix.
\def\tp@create@editor@string#1{%
  \expandafter\ifx\csname tp@\tp@cur@cont @#1NameList\endcsname\relax\else
    \csgappto{tp@\tp@cur@cont @#1NameList}{{\letcs\tpTotalCount{tp#1Cnt}~\tpUseProperty{editor-suffix}}}%
  \fi
}%

